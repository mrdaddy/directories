CREATE TABLE ETICKET.PARAMETERS (
  ID           INTEGER GENERATED BY DEFAULT AS IDENTITY (START WITH 1, INCREMENT BY 1) NOT NULL CONSTRAINT PARAMETERS_PK1 PRIMARY KEY,
  TYPE         CHAR(1) NOT NULL,
  CODE         VARCHAR(32) NOT NULL,
  VALUE        VARCHAR(1024),
  UPDATE_TIME   TIMESTAMP,
  IS_API_PROPAGATED CHAR(1) CHECK( IS_API_PROPAGATED='1' OR IS_API_PROPAGATED = '0' ) DEFAULT '1')