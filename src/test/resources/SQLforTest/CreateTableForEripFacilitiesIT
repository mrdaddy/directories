CREATE TABLE ETICKET.ERIP_PAYMENT_FACILITIES (
             ID            INTEGER GENERATED BY DEFAULT AS IDENTITY (START WITH 1, INCREMENT BY 1) NOT NULL CONSTRAINT ERIP_PF_PK1 PRIMARY KEY,
             FACILITY_TYPE VARCHAR(16) NOT NULL,
             FACILITY_NAME VARCHAR(256) NOT NULL,
             FACILITY_URL  VARCHAR(256),
             UPDATE_TIME TIMESTAMP)
