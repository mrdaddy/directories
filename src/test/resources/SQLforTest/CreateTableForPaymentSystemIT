CREATE TABLE ETICKET.PAYMENT_SYSTEMS (
             ID                      INTEGER GENERATED BY DEFAULT AS IDENTITY (START WITH 1, INCREMENT BY 1) NOT NULL CONSTRAINT PS_PK1 PRIMARY KEY,
             PAYMENT_SYSTEM_TYPE     VARCHAR(16) NOT NULL,
             PAYMENT_SYSTEM_NAME_RU	 VARCHAR(256) NOT NULL DEFAULT '',
             PAYMENT_SYSTEM_NAME_EN	 VARCHAR(256) NOT NULL DEFAULT '',
             PAYMENT_SYSTEM_SN_RU	   VARCHAR(128) NOT NULL DEFAULT '',
             PAYMENT_SYSTEM_SN_EN	   VARCHAR(64) NOT NULL DEFAULT '',
             IS_DEFAULT              CHAR(1) NOT NULL DEFAULT '0',
             IS_ACCESSIBLE_RU        CHAR(1) NOT NULL DEFAULT '1',
             IS_ACCESSIBLE_EN        CHAR(1) NOT NULL DEFAULT '1',
             PAYMENT_SYSTEM_URL      VARCHAR(256),
             PAYMENT_TIME            INTEGER NOT NULL,
             ETICKET_CODE            VARCHAR(16),
             ETICKET_NAME            VARCHAR(256),
             ETICKET_URL             VARCHAR(256),
             ETICKET_CANCEL_URL      VARCHAR(256),
             ADDITIONAL_PARAMS       VARCHAR(1024),
             PAYING_AGENT            CHAR(4),
             IS_TICKET_RETURN_ENABLE CHAR(1) NOT NULL DEFAULT '0',
             IS_ONLINE_RETURN_ENABLE CHAR(1) NOT NULL DEFAULT '0',
             UPDATE_TIME             TIMESTAMP)